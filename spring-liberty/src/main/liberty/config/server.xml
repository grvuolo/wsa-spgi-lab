<?xml version="1.0" encoding="UTF-8"?>
<server description="new server">

    <featureManager>
        <feature>servlet-6.0</feature>
        <feature>springBoot-3.0</feature>
        <feature>usageMetering-1.0</feature>
        <feature>transportSecurity-1.0</feature>
    </featureManager>

    <httpEndpoint id="defaultHttpEndpoint"
                  host="*"
                  httpPort="9080"
                  httpsPort="9443" />

    <keyStore id="meteringKeyStore" password="changeMe" location="key.p12" type="PKCS12" />
    <ssl id="meteringSSL" keyStoreRef="meteringKeyStore" trustStoreRef="meteringKeyStore" sslProtocol="TLSv1.2" />

    <springBootApplication id="guide-spring-boot" 
                           location="thin-guide-spring-boot-0.1.0.jar"
                           name="guide-spring-boot" />

    <usageMetering url="https://cpd-websphere-automation.apps.ocp.ibm.edu/websphereauto/meteringapi" 
                   sslRef="meteringSSL" 
                   apiKey="" />

</server>
